{
  "name": "couper",
  "displayName": "Couper Configuration",
  "version": "0.0.1",
  "publisher": "couper.io",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "couper",
        "extensions": ["hcl", "couper"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "couper",
        "scopeName": "source.couper",
        "path": "./syntax/couper.json"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "main": "src/extension.js",
  "scripts": {
    "watch": "true"
  },
  "devDependencies": {}
}
